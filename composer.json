{
    "name": "rokfor/rokfor-slim",
    "type": "cms",
    "description": "Admin Backend for a Rokfor Database built on slim using bootstrap, adminLte and jade",
    "keywords": ["rokfor", "slim","framework","view","template","jade"],
    "homepage": "http://www.rokfor.ch",
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Urs Hofer",
            "email": "inquiries@rokfor.ch",
            "homepage": "http://www.urshofer.ch"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/urshofer/slim-jade"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Rokfor/rokfor-php-db"
        },
        {
            "type": "vcs",
            "url": "https://github.com/urshofer/slim-auth"
        },
        {
            "type": "vcs",
            "url": "https://github.com/urshofer/json-editor"
        },
        {
            "type": "vcs",
            "url": "https://github.com/urshofer/jquery-locationpicker-plugin"
        },
        {
            "type": "vcs",
            "url": "https://github.com/urshofer/uploader"
        },
        {
            "type": "vcs",
            "url": "https://github.com/urshofer/range2dslider"
        },
        {
            "type": "vcs",
            "url": "https://github.com/urshofer/select2sortable"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "slim/slim": "~3.0",
        "jlndk/slim-jade": "dev-master",
        "rokfor/db": "dev-master",
        "urshofer/select2sortable": "dev-master",
        "logicify/jquery-locationpicker-plugin": "dev-master",
        "jdorn/json-edit": "dev-master",
        "xdan/range2dslider": "dev-master",
        "danielm/uploader": "dev-master",
        "summernote/summernote": "^0.7.3", 
        "monolog/monolog": "^1.17",
        "slim/csrf": "^0.6.0",
        "jeremykendall/slim-auth": "dev-slim-3.x",
        "slim/flash": "^0.1.0",
        "akrabat/rka-ip-address-middleware": "^0.4.0",
        "almasaeed2010/adminlte": "~2.3.2",
        "fortawesome/font-awesome": "^4.4.0",
        "robinherbots/jquery.inputmask": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "scripts": {
          "post-install-cmd": [
              "vendor/propel/propel/bin/propel sql:insert --config-dir vendor/rokfor/db/config"
          ]
    }
}
